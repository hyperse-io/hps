{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "allowJs": false,
    "noImplicitAny": true,
    "noEmit": false,
    "incremental": true,
    "paths": {
      "@hyperse/hps-srv-common": ["../../packages/hps-srv-common/src/index.ts"],
      "@hyperse/hps-srv-mock": ["../../packages/hps-srv-mock/src/index.ts"],
      "@hyperse/hps-srv-rspack-plugin-import": [
        "../../packages/hps-srv-rspack-plugin-import/src/index.ts"
      ]
    },
    "types": ["vitest/globals"]
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["**/node_modules", "**/.*/", "dist", "build"]
}
